@model IEnumerable<InstitutoEducativo.Models.Calificacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NotaFinal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Profesor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NotaFinal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Profesor.Apellido)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CalificacionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CalificacionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CalificacionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
